{
  "name": "funny-blog",
  "version": "0.0.1",
  "scripts": {
    "start":"npm run pull && npm run build && npm run serve",
    "dev": "node server",
    "build": "npm run build:client && npm run build:server ",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --ssrManifest --outDir dist/client --mode prod",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server --mode prod",
    "build:server:noExternal": "vite build --config vite.config.noexternal.js --ssr src/entry-server.js --outDir dist/server --mode prod",
    "generate": "vite build --ssrManifest --outDir dist/static --mode prod && npm run build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server",
    "pull":"git fetch --all && git reset --hard origin/v2.0"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@types/md5": "^2.3.1",
    "ant-design-vue": "^2.2.8",
    "axios": "^0.21.1",
    "chalk": "^4.1.2",
    "crypto-js": "^4.1.1",
    "swagger-vue": "^1.0.3",
    "vue": "^3.2.26",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@types/crypto-js": "^4.0.2",
    "@types/node": "^16.7.1",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.5",
    "acorn": "^8.5.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "rollup-plugin-external-import": "^1.0.9",
    "rollup-plugin-visualizer": "^5.5.2",
    "serve-static": "^1.14.2",
    "typescript": "^4.3.2",
    "unplugin-vue-components": "^0.17.9",
    "vite": "^2.4.4",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-compression": "^0.3.5",
    "vue-cli-plugin-vue-next": "~0.1.4",
    "vue-tsc": "^0.2.2"
  },
  "description": "This template should help get you started developing with Vue 3 and Typescript in Vite.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enncy/funny-blog.git"
  },
  "keywords": [
    "blog",
    "springboot",
    "vue3",
    "vue-router",
    "mysql",
    "ant-design-vue"
  ],
  "author": "enncy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/enncy/funny-blog/issues"
  },
  "homepage": "https://github.com/enncy/funny-blog#readme"
}
